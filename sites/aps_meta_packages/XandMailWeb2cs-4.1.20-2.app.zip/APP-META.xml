		<group>
			<name>Mail preferences</name>
			<setting id="imap_folder_name_draft" type="string" default-value="draft" installation-only="true">
				<name>Draft IMAP folder name</name>
				<description>Please provide the IMAP folder name for the draft messages.</description>
			</setting>
			<setting id="imap_folder_name_outbox" type="string" default-value="outbox" installation-only="true">
				<name>Sent IMAP folder name</name>
				<description>Please provide the IMAP folder name for the sent messages.</description>
			</setting>
			<setting id="imap_folder_name_trash" type="string" default-value="trash" installation-only="true">
				<name>Trash IMAP folder name</name>
				<description>Please provide the IMAP folder name for the deleted messages.</description>
			</setting>
			<setting id="imap_folder_name_spam" type="string" default-value="Spam" installation-only="true">
				<name>Spam IMAP folder name</name>
				<description>Please provide the IMAP folder name for the spam messages.</description>
			</setting>
			<setting id="imap_host" type="string" default-value="localhost" installation-only="true">
				<name>IMAP Host</name>
				<description>Please provide the name or IP address of the IMAP host.</description>
			</setting>
			<setting id="imap_host_port" type="string" default-value="143" installation-only="true">
				<name>IMAP Port</name>
				<description>Please provide the port number to access the IMAP host.</description>
			</setting>
<!--
			<setting id="imap_host_port_ssl" type="string" default-value="993" installation-only="true">
				<name>IMAP SSL Port</name>
				<description>Please provide the port number to access the IMAP host in SSL mode.</description>
			</setting>
-->
			<setting id="imap_login_long" type="enum" default-value="yes" installation-only="true">
				<name>Names for POP3/IMAP mail accounts</name>
				<description>This application has been configured to run together with the Courier IMAP4 server, using full POP3/IMAP4 mail account names. If this default configuration was changed in the Server-wide mail preferences to allow both short and full names, change this value to 'short and full'. If it was not changed or you do not know, leave this value set to 'Full'.</description>
				<choice id="yes">
					<name>full</name>
				</choice>
				<choice id="no">
					<name>short and full</name>
				</choice>
			</setting>
			<setting id="smtp_host" type="string" default-value="localhost" installation-only="true">
				<name>SMTP Host</name>
				<description>Please provide the name or IP address of the SMTP host.</description>
			</setting>
			<setting id="smtp_host_port" type="string" default-value="25" installation-only="true">
				<name>SMTP Port</name>
				<description>Please provide the port number to access the SMTP host.</description>
			</setting>
		</group>
	</settings>
	<requirements
		xmlns:php="http://apstandard.com/ns/1/php"
		xmlns:apache="http://apstandard.com/ns/1/apache"
		xmlns:mail="http://apstandard.com/ns/1/mail">
		<php:version min="4.3.9"/>
		<php:safe-mode>false</php:safe-mode>
		<apache:required-module>mod_cgi</apache:required-module>
	</requirements>
	<mapping url="/" path="htdocs" xmlns:php="http://apstandard.com/ns/1/php" xmlns:cgi="http://apstandard.com/ns/1/cgi" xmlns:h="http://apstandard.com/ns/1/cgi/handlers">
		<mapping url="cgi-bin" xmlns:cgi="http://apstandard.com/ns/1/cgi" xmlns:h="http://apstandard.com/ns/1/cgi/handlers">
			<cgi:handler>
				<cgi:all-files/>
			</cgi:handler>
		</mapping>
		<mapping url="admin">
			<php:handler>
				<php:extension>php</php:extension>
			</php:handler>
		</mapping>
		<mapping url="bin">
			<mapping url="rss_ticker">
				<php:permissions writable="true"/>
			</mapping>
		</mapping>
		<mapping url="data">
			<php:permissions writable="true"/>
		</mapping>
		<mapping url="ext_cloud">
			<mapping url="classic">
				<mapping url="themes">
					<mapping url="blue">
						<mapping url="css">
							<php:permissions writable="true"/>
						</mapping>
					</mapping>
				</mapping>
			</mapping>
		</mapping>
		<mapping url="include">
			<php:permissions writable="true"/>
		</mapping>
		<mapping url="upload">
			<php:permissions writable="true"/>
		</mapping>
		<mapping url="web_imgs">
			<mapping url="xandmail">
				<php:permissions writable="true"/>
			</mapping>
		</mapping>
		<mapping url="xam_log">
			<mapping url="default">
				<php:permissions writable="true"/>
			</mapping>
		</mapping>
		<mapping url="xam_tmp">
			<php:permissions writable="true"/>
		</mapping>
		<mapping url="domains">
			<php:permissions writable="true"/>
		</mapping>
	</mapping>
</application>
